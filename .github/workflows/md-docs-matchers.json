{
    "problemMatcher": [
        {
            "owner": "markdown_linter_warnings",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^(.*):(\\d+):\\s(MD\\d{3})\\s(.*)$",
                    "file": 1,
                    "line": 2,
                    "message": 4,
                    "code": 3
                }
            ]
        }
    ]
}
